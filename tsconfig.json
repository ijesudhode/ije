{
    "compilerOptions": {
        "target": "ES2022",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "outDir": "./dist",
        "rootDir": ".",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "resolveJsonModule": true,
        "noImplicitAny": false,
        "noEmitOnError": false
    },
    "include": [
        "./*.ts",
        "./runtime/*.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}